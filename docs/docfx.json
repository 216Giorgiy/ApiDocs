{
  "metadata": [{
    "src": [{
      "files": [ 
        "**/src/**/project.json"
      ],
      "exclude": [
        "../aspnet/razor/src/Microsoft.AspNet.Razor.Runtime.VSRC1/*",
        "../aspnet/razor/src/Microsoft.AspNet.Razor.VSRC1/*",
        "../aspnet/mvc/src/Microsoft.AspNet.Mvc.Razor.Host.VSRC1/*",
        "../aspnet/filesystem/src/Microsoft.AspNet.FileProviders.Abstractions.VSRC1/*",
        "../aspnet/filesystem/src/Microsoft.AspNet.FileProviders.Physical.VSRC1/*",
        "../aspnet/caching/src/Microsoft.Extensions.Caching.Abstractions.VSRC1/*",
        "../aspnet/caching/src/Microsoft.Extensions.Caching.Memory.VSRC1/*",
        "../aspnet/options/src/Microsoft.Extensions.OptionsModel.VSRC1/*"
      ],
      "src": "../aspnet"
    }],
    "dest": "api/"
  }],
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "dest": "."
      },
      {
        "files": [
          "**/*.yml"
        ],
        "dest": "."
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "breadcrumb.json"
        ],
        "exclude": [
          "**/obj/**",
          "aspnet/api/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/aspnetcore/api/breadcrumb.json"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "aspnet/api"
  }
}